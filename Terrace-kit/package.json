		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@codemirror/lang-markdown": "^6.1.0",
		"@codemirror/theme-one-dark": "^6.1.1",
		"svelte-codemirror-editor": "^1.1.0",
		"svelte-exmarkdown": "^2.0.0",
		"svelte-highlight": "^7.2.0"
	}
}
